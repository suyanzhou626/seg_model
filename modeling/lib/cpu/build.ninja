ninja_required_version = 1.3
cxx = c++

cflags = -DTORCH_EXTENSION_NAME=enclib_cpu -I/home/SENSETIME/wuyao/anaconda3/lib/python3.7/site-packages/torch/lib/include -I/home/SENSETIME/wuyao/anaconda3/lib/python3.7/site-packages/torch/lib/include/TH -I/home/SENSETIME/wuyao/anaconda3/lib/python3.7/site-packages/torch/lib/include/THC -I/home/SENSETIME/wuyao/anaconda3/include/python3.7m -D_GLIBCXX_USE_CXX11_ABI=0 -fPIC -std=c++11
ldflags = -shared

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out
  depfile = $out.d
  deps = gcc

rule link
  command = $cxx $in $ldflags -o $out

build operator.o: compile /mnt/lustre/wuyao/ours_network/seg_model/modeling/lib/cpu/operator.cpp
build encoding_cpu.o: compile /mnt/lustre/wuyao/ours_network/seg_model/modeling/lib/cpu/encoding_cpu.cpp
build syncbn_cpu.o: compile /mnt/lustre/wuyao/ours_network/seg_model/modeling/lib/cpu/syncbn_cpu.cpp
build roi_align_cpu.o: compile /mnt/lustre/wuyao/ours_network/seg_model/modeling/lib/cpu/roi_align_cpu.cpp
build nms_cpu.o: compile /mnt/lustre/wuyao/ours_network/seg_model/modeling/lib/cpu/nms_cpu.cpp

build enclib_cpu.so: link operator.o encoding_cpu.o syncbn_cpu.o roi_align_cpu.o nms_cpu.o

default enclib_cpu.so

